//Program to display 5 times Hello on screen

#include<stdio.h>

//Demonstration of ITERATION

void Display()
{
	int icnt=0;
/*	
	1 = Initialization           
	2 = Condition				 
	3 = Displacement
	4 = Loop body
*/
	//	   1	  2 	   3	   	
for ( icnt=-2;  icnt<=5;  icnt++)		
	{
 	    printf("Hello\n");  //  4
	}

}

int main()
{
	Display();     
	
	return 0;
}


